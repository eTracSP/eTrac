                            <div class="img"><img src="~/Content/Images/Report/TrashLevel.png"></div>
                            <div class="info">
                                <h5>Issued Item</h5>
                            </div>
                        </a>
                    </div>
                </div>*@
            <div class="row-2">
                <div class="item-hover circle colored effect3 left_to_right">
                    @*<a href="../Report/Reports?ReportName=Assets Work Order">*@
                        <a href="@Url.Action("AssetsWorkOrder", "Report")">
                            <div class="img"><img src="~/Content/Images/Report/checklist-3.png"></div>
                            <div class="info">
                                <h5>Assets Work Order</h5>
                            </div>
                        </a>
                </div>
            </div>
            @*<div class="row-2">
                    <div class="item-hover circle colored effect3 left_to_right">
                        <a href="../Report/Reports?ReportName=Trash Picked Up Details">
                            <div class="img"><img src="~/Content/Images/Report/safetyhzd.jpg" alt="img"></div>
