@{
    ViewBag.Title = "My Profile";
    Layout = "~/Views/Shared/_ManagerLayout.cshtml";

}
@using (Html.BeginForm("ManagerProfile", "Manager", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="panel panel-default">
        <div class="panel-heading">
            Personal Details
        </div>
        @Html.Partial("_Registration")
        <div class="marbottom"></div>
        <div class="col-sm-offset-9">
            <button class="btn btn-default bluebutton" type="submit">Save</button>
            <a href="~/GlobalAdmin/Login" class="btn btn-default bluebutton">Cancel</a>
            <div class="clearfix"></div>
        </div>
        <div class="marbottom"></div>
    </div>
}
@section scripts{
    @Scripts.Render("~/bundles/jqueryval")
}