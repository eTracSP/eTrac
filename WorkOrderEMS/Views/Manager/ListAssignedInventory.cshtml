
@{
    ViewBag.Title = "ListAssignedInventory";
    Layout = "~/Views/Shared/_ManagerLayout.cshtml";
    long ProjectID = 0;
    if (ViewBag.ProjectID != null)
    {
        ProjectID = Convert.ToInt32(ViewBag.ProjectID);
    }
}
<script type="text/javascript">
    $_ProjectID = '@ProjectID';
    function exportPDF()
    {
        document.location.href = '/Manager/ListAssignedInventory?pdf=True&ProjectID=' + $_ProjectID;
    }
</script>
<script src="~/GridListing/JqGridScript/AssignedInventoryList.js"></script>
<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
    <div id="jqGrid_container">
        <table id="tbl_AssignedInventoryList" border="0" cellpadding="0" cellspacing="0"></table>
        <div id="divAssignedInventoryListPager">
        </div>
    </div>
</div>


